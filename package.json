{
	"name": "symplex",
	"displayName": "Symplex",
	"description": "Symbolic Manipulation for VS Code",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.33.0"
	},
	"repository": { 
		"type": "git", 
		"url": "https://github.com/zalo/Symplex.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.Evaluate",
		"onCommand:extension.GetExpression"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.Evaluate",
				"title": "Symplex: Symbolic Evaluate"
			},
			{
				"command": "extension.GetExpression",
				"title": "Symplex: Get Expression"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection && resourceLangId =~ /(javascript|typescript|python)/",
					"command": "extension.Evaluate",
					"group": "navigation@0"
				},
				{
					"when": "editorHasSelection && resourceLangId =~ replacewiththeabovetodebugparsing",
					"command": "extension.GetExpression",
					"group": "navigation@1"
				}
			]
		},
		"snippets": [
      {
        "language": "javascript",
        "path": "./snippets.json"
			},
			{
        "language": "typescript",
        "path": "./snippets.json"
			},
			{
        "language": "python",
        "path": "./snippets.json"
      }
    ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"typescript": "^3.1.1"
	}
}
