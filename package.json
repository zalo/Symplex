{
	"name": "symplex",
	"displayName": "Symplex",
	"description": "Symbolic Manipulation for VS Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.Differentiate",
		"onCommand:extension.Integrate",
		"onCommand:extension.FindExtrema",
		"onCommand:extension.Simplify"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.Differentiate",
				"title": "Differentiate"
			},
			{
				"command": "extension.Integrate",
				"title": "Integrate"
			},
			{
				"command": "extension.FindExtrema",
				"title": "FindExtrema"
			},
			{
				"command": "extension.Simplify",
				"title": "Simplify"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "extension.Differentiate",
					"group": "Symplex@1"
				},
				{
					"when": "editorHasSelection",
					"command": "extension.Integrate",
					"group": "Symplex@1"
				},
				{
					"when": "editorHasSelection",
					"command": "extension.FindExtrema",
					"group": "Symplex@1"
				},
				{
					"when": "editorHasSelection",
					"command": "extension.Simplify",
					"group": "Symplex@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"typescript": "^3.1.1"
	}
}
